<html>
<head>
    <meta charset="UTF-8">
    <title> {{ full_name }} - {{ details.title }} </title>

    <style>
        {{ css }}
    </style>
</head>
<body>

    <div class="cv-wrapper">

        <div class="header">
            <table>
                <tr>
                    <td width="100%">
                        <p class="name">
                            <span class="cyan">{{ details.last_name }}</span>
                            <span>{{ details.first_name }}</span>
                        </p>
                    </td>
                    <td class="content-wrapper">
                        <ul class="contact-list">
                            {{# contact_padding}}
                                <li class="contact-wrapper">&nbsp;</li>
                            {{/ contact_padding}}
                            {{# contact}}
                                <li class="contact-wrapper">
                                    <a {{# url}}href="{{ url }}"{{/ url}}>{{{icon}}}{{ text }}</a>
                                </li>
                            {{/ contact}}
                        </ul>
                    </td>
                </tr>
            </table>
            <hr>
            <p class="title">{{ details.title }}</p>
        </div>

        <table>

<!--
____________ ___________ _____ _      _____ 
| ___ \ ___ \  _  |  ___|_   _| |    |  ___|
| |_/ / |_/ / | | | |_    | | | |    | |__  
|  __/|    /| | | |  _|   | | | |    |  __| 
| |   | |\ \\ \_/ / |    _| |_| |____| |___ 
\_|   \_| \_|\___/\_|    \___/\_____/\____/
-->
            <tr class="section">
                <td class="title-wrapper">
                    <p class="title cyan">Profile</p>
                </td>
                <td class="content-wrapper">
                    <p class="content">{{ profile }}</p>
                </td>
            </tr>

<!--
 _____ _   _______ _      _      _____ 
/  ___| | / /_   _| |    | |    /  ___|
\ `--.| |/ /  | | | |    | |    \ `--. 
 `--. \    \  | | | |    | |     `--. \
/\__/ / |\  \_| |_| |____| |____/\__/ /
\____/\_| \_/\___/\_____/\_____/\____/
-->
            <tr class="section">
                <td class="title-wrapper">
                    <p class="title cyan">Skills</p>
                </td>
                <td class="content-wrapper">
                    <div class="fields-wrapper">
                        <ul class="fields-list skill-list no-list" style="columns: {{ skills.columns }};">
                            {{# skills.fields}}
                                <li>
                                    <span class="skill-title">{{ name }}</span>
                                    <br>
                                    <span class="content">{{ description }}</span>
                                </li>
                            {{/ skills.fields}}
                        </ul>
                    </div>
                </td>
            </tr>

<!--
 _____ _____ _____  _   _  _   _ _____ _____   ___   _     
|_   _|  ___/  __ \| | | || \ | |_   _/  __ \ / _ \ | |    
  | | | |__ | /  \/| |_| ||  \| | | | | /  \// /_\ \| |    
  | | |  __|| |    |  _  || . ` | | | | |    |  _  || |    
  | | | |___| \__/\| | | || |\  |_| |_| \__/\| | | || |____
  \_/ \____/ \____/\_| |_/\_| \_/\___/ \____/\_| |_/\_____/
-->
            <tr class="section">
                <td class="title-wrapper">
                    <p class="title cyan">Technical</p>
                </td>
                <td class="content-wrapper">
                    <ul class="skill-list no-list">
                        {{# technical}}
                            <li>
                                <span class="skill-title">{{ category }}:</span>
                                <span class="content">{{ items }}</span>
                            </li>
                        {{/ technical}}
                    </ul>
                </td>
            </tr>

<!--
_______   ________ ___________ _____ _____ _   _ _____  _____ 
|  ___\ \ / /| ___ \  ___| ___ \_   _|  ___| \ | /  __ \|  ___|
| |__  \ V / | |_/ / |__ | |_/ / | | | |__ |  \| | /  \/| |__  
|  __| /   \ |  __/|  __||    /  | | |  __|| . ` | |    |  __| 
| |___/ /^\ \| |   | |___| |\ \ _| |_| |___| |\  | \__/\| |___ 
\____/\/   \/\_|   \____/\_| \_|\___/\____/\_| \_/\____/\____/                                        
-->
            <tr class="section">
                <td class="title-wrapper">
                    <p class="title cyan">Work Experience</p>
                </td>
                <td class="content-wrapper">
                    {{# experience}}
                        <table class="subsection experience">
                            <tr>
                                <td>
                                    <p class="title">
                                        {{ company }}
                                    </p>
                                </td>
                                <td class="period-wrapper">
                                    <p>
                                        {{ from.month }} <b>{{ from.year }}</b> -
                                            {{# to}}{{ to.month }} <b>{{ to.year }}</b>{{/ to}}
                                            {{^ to}}Present{{/ to}}
                                    </p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p class="subtitle">
                                        {{ position }}
                                    </p>
                                </td>
                                <td class="location-wrapper">
                                    <p class="cyan">{{ location }}</p>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <ul class="no-list">
                                        {{# description}}
                                            <li class="content">{{& . }}</li>
                                        {{/ description}}
                                    </ul>
                                </td>
                            </tr>
                        </table>
                    {{/ experience}}
                </td>
            </tr>

<!--
___________ _   _ _____   ___ _____ _____ _____ _   _ 
|  ___|  _  \ | | /  __ \ / _ \_   _|_   _|  _  | \ | |
| |__ | | | | | | | /  \// /_\ \| |   | | | | | |  \| |
|  __|| | | | | | | |    |  _  || |   | | | | | | . ` |
| |___| |/ /| |_| | \__/\| | | || |  _| |_\ \_/ / |\  |
\____/|___/  \___/ \____/\_| |_/\_/  \___/ \___/\_| \_/
-->
            <tr class="section">
                <td class="title-wrapper">
                    <p class="title cyan">Education</p>
                </td>
                <td class="content-wrapper">
                    {{# education}}
                        <table class="subsection education">
                            <tr>
                                <td>
                                    <p class="title">
                                        {{# degree}}<span class="degree">{{ degree }}</span> | {{/ degree}}
                                        {{ field }}
                                    </p>
                                </td>
                                <td class="period-wrapper">
                                    <p>{{ from.month }} <b>{{ from.year }}</b> - {{ to.month }} <b>{{ to.year }}</b></p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p class="subtitle">
                                        {{ school }}
                                    </p>
                                </td>
                                <td class="location-wrapper">
                                    <p class="cyan">{{ location }}</p>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <ul class="no-list">
                                        {{# description}}
                                            <li class="content">{{& . }}</li>
                                        {{/ description}}
                                    </ul>
                                </td>
                            </tr>
                        </table>
                    {{/ education}}
                </td>
            </tr>
        </table>

    </div>

</body>
</html>